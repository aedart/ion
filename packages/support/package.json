{
    "name": "@aedart/support",
    "version": "0.6.1",
    "description": "The Ion support package",
    "keywords": [
        "Support",
        "Utilities"
    ],
    "author": "Alin Eugen Deac <aedart@gmail.com>",
    "license": "BSD-3-Clause",
    "homepage": "https://aedart.github.io/ion/",
    "bugs": {
        "url": "https://github.com/aedart/ion/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aedart/ion.git",
        "directory": "packages/support"
    },
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/types/support.d.ts",
            "import": "./dist/esm/support.js",
            "require": "./dist/cjs/support.cjs"
        },
        "./meta": {
            "types": "./dist/types/meta.d.ts",
            "import": "./dist/esm/meta.js",
            "require": "./dist/cjs/meta.cjs"
        },
        "./misc": {
            "types": "./dist/types/misc.d.ts",
            "import": "./dist/esm/misc.js",
            "require": "./dist/cjs/misc.cjs"
        },
        "./objects": {
            "types": "./dist/types/objects.d.ts",
            "import": "./dist/esm/objects.js",
            "require": "./dist/cjs/objects.cjs"
        },
        "./reflections": {
            "types": "./dist/types/reflections.d.ts",
            "import": "./dist/esm/reflections.js",
            "require": "./dist/cjs/reflections.cjs"
        }
    },
    "files": [
        "dist",
        "!dist/**/*.map"
    ],
    "peerDependencies": {
        "@aedart/contracts": "^0.6.1",
        "@types/lodash-es": "^4.17.7",
        "lodash-es": "^4.17.21"
    },
    "scripts": {
        "compile": "rollup -c",
        "watch": "rollup -c -w"
    },
    "sideEffects": false
}
