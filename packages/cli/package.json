{
    "name": "@aedart/cli",
    "version": "0.13.0",
    "description": "Ion CLI - Development tools for applications that make use of Ion",
    "keywords": [
        "CLI",
        "Console",
        "Tools",
        "Ion"
    ],
    "author": "Alin Eugen Deac <aedart@gmail.com>",
    "license": "BSD-3-Clause",
    "homepage": "https://aedart.github.io/ion/",
    "bugs": {
        "url": "https://github.com/aedart/ion/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aedart/ion.git",
        "directory": "packages/cli"
    },
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "type": "module",
    "exports": {
        ".": {
