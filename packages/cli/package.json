    ],
    "engines": {
        "node": "^22.9.0"
    },
    "peerDependencies": {
        "@aedart/contracts": "^0.13.0",
        "@aedart/support": "^0.13.0",
        "@aedart/core": "^0.13.0",
        "commander": "^12.1.0",
        "inquirer": "^12.0.0",
        "chalk": "^5.3.0"
    },
    "scripts": {
        "compile": "rollup -c",
        "watch": "rollup -c -w"
    },
    "sideEffects": false
