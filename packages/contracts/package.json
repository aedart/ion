{
    "name": "@aedart/contracts",
    "version": "0.5.0",
    "description": "The Ion contracts package. Contains types, interfaces and unique identifiers",
    "keywords": [
        "Ion",
        "Contracts",
        "Interfaces",
        "Types",
        "Unique Identifiers"
    ],
    "author": "Alin Eugen Deac <aedart@gmail.com>",
    "license": "BSD-3-Clause",
    "homepage": "https://aedart.github.io/ion/",
    "bugs": {
        "url": "https://github.com/aedart/ion/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aedart/ion.git",
        "directory": "packages/contracts"
    },
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/types/contracts.d.ts",
            "import": "./dist/esm/contracts.js",
            "require": "./dist/cjs/contracts.cjs"
        },
        "./support": {
            "types": "./dist/types/support.d.ts",
            "import": "./dist/esm/support.js",
            "require": "./dist/cjs/support.cjs"
        },
        "./support/meta": {
            "types": "./dist/types/support/meta.d.ts",
            "import": "./dist/esm/support/meta.js",
            "require": "./dist/cjs/support/meta.cjs"
        }
    },
    "files": [
        "dist",
        "!dist/**/*.map"
    ],
    "scripts": {
        "compile": "rollup -c",
        "watch": "rollup -c -w"
    },
    "sideEffects": false
}
