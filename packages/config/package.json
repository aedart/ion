{
    "name": "@aedart/config",
    "version": "0.13.0",
    "description": "Ion Configuration package",
    "keywords": [
        "Configuration",
        "Ion"
    ],
    "author": "Alin Eugen Deac <aedart@gmail.com>",
    "license": "BSD-3-Clause",
    "homepage": "https://aedart.github.io/ion/",
    "bugs": {
        "url": "https://github.com/aedart/ion/issues"
    },
    "repository": {
